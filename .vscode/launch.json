  "version": "0.2.0",
  "configurations": [
    {
      "name": ".NET Core Launch (generator)",
      "type": "coreclr",
      "request": "launch",
      "preLaunchTask": "build-generator",
      "program": "${workspaceFolder}/generator/bin/Debug/net7.0/generator",
      "args": [],
      "cwd": "${workspaceFolder}/generator/bin/Debug/net7.0",
      "stopAtEntry": false,
      "console": "internalConsole"
    },
    {
      "name": ".NET Core Attach",
      "type": "coreclr",
      "request": "attach"
    }
  ]
}
